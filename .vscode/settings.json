{
  "vsicons.dontShowNewVersionMessage": true,
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.tabSize": 2,
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[vue]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "eslint.format.enable": true,
  "path-intellisense.mappings": {
    "/": "${workspaceFolder}",
    "src": "${workspaceFolder}/src",
    "components": "${workspaceFolder}/src/components",
    "layouts": "${workspaceFolder}/src/layouts",
    "assets": "${workspaceFolder}/src/assets",
    "boot": "${workspaceFolder}/src/boot",
    "pages": "${workspaceFolder}/src/pages"
  },
  "path-autocomplete.pathMappings": {
    "src": "${folder}/src",
    "components": "${folder}/src/components",
    "layouts": "${folder}/src/layouts",
    "assets": "${folder}/src/assets",
    "boot": "${folder}/src/boot",
    "pages": "${folder}/src/pages"
  },
  "files.eol": "\n",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "explorer.confirmDragAndDrop": false,
  "vetur.format.defaultFormatterOptions": {
    "prettier": {
      "singleQuote": true //‰ΩøÁî®ÂçïÂºïÂè∑ËÄå‰∏çÊòØÂèåÂºïÂè∑
    },
    "js-beautify-html": {
      "end_with_newline": false
    },
    "prettyhtml": {
      "printWidth": 100,
      "singleQuote": false,
      "wrapAttributes": false,
      "sortAttributes": false
    }
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vetur.format.defaultFormatter.html": "prettyhtml",
  "prettier.singleQuote": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "javascript.referencesCodeLens.showOnAllFunctions": true,
  "typescript.referencesCodeLens.enabled": true,
  "editor.guides.indentation": false,
  "editor.guides.bracketPairs": false,
  "cSpell.userWords": [
    "Alarge",
    "ATAC",
    "autofix",
    "BIGDATA",
    "borderless",
    "browserslist",
    "cellnumber",
    "cngb",
    "cngbdb",
    "codeplot",
    "connectdevelop",
    "cwlts",
    "cword",
    "deflist",
    "duwensi",
    "echarts",
    "gtag",
    "Invisibles",
    "jschardet",
    "katex",
    "merriweather",
    "micos",
    "nofollow",
    "omics",
    "PBMC",
    "prismjs",
    "rabix",
    "stomics",
    "stylelint",
    "stylelintrc",
    "suwei",
    "trueblood",
    "Tureblood",
    "vuelidate",
    "vuex",
    "wangzhijie",
    "yangyawen",
    "zoomin"
  ],
  "vetur.validation.interpolation": false,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false,
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.unicodeHighlight.includeComments": false,
  "vetur.completion.scaffoldSnippetSources": {
    "workspace": "üíº",
    "user": "üóíÔ∏è",
    "vetur": "‚úå"
  },
  "editor.formatOnSave": true
}
